# Конфигурация подключения к базе данных PostgreSQL
DB_HOST=postgres
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=at_scheduler
DB_SSLMODE=disable

# Настройки Worker
# WORKER_ID - идентификатор для логов (опционально)
# Если не задан, автоматически используется hostname контейнера
# Это полезно для docker-compose --scale
#WORKER_ID=worker-1
WORKER_POLLING_INTERVAL=5
WORKER_BATCH_SIZE=10
WORKER_CLEANER_INTERVAL=5
WORKER_STUCK_TIMEOUT=5
